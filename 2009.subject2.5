#include <iostream>
#include <algorithm>
using namespace std;

int main()
{
int i;
string str,str2;
cin>>str;
int pos= 'a' - 'A';
for(i=0;i<str.size();i++){
    if(str[i]=='a'||str[i]=='e'||str[i]=='i'||str[i]=='o'||str[i]=='u'){
        str2+=str[i];
        str2+=(str[i]-pos);
    }else{
    str2+=str[i];}
}
cout << str2 << endl ;
return 0 ;

}
