paros szamokat dublaza ,ha nincs ilyen kiier =-1

void duplicare(int n, int &d)

{

    int p=1,x=n;

    d=0;

    while (x)
    {
        if(x%10%2) d+=x%10*p;
}
        else

        {

            d+=x%10*p;

            p*=10;

            d+=x%10*p;

        }

        x/=10;

        p*=10;

    }

    if(d==n) 
    {
    d=-1;
}
}
